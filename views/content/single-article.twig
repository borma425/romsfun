


{% extends "base.twig" %}

{% block grid_content %}



{% include 'main/html-header.twig' %}






<div id="content" class="site-content">
	
	
					<div class="bg-light py-2">
				<div class="container">
					
    <ul id="breadcrumb" class="breadcrumb">
        <li class="breadcrumb-item home"><a href="{{ site.link   }}" title="Home">Home</a></li>

        <li class="breadcrumb-item item-cat"><a href="{{ current_cpt_link }}">{{ current_cpt_type }}</a></li> 
        <li class="breadcrumb-item active">{{ post.title   }}</li> 

                </ul>
				</div>
			</div>
		


		
		<div class="container py-4">
    <main id="primary" class="content-area">
        <h1 class="h3 font-weight-bold">{{ post.title   }}</h1>
        <time class="text-muted d-flex align-items-center mb-3" datetime="June 11, 2024">
            <svg class="flex-shrink-0 mr-1" fill="#999" height="14px" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M256 8C119 8 8 119 8 256s111 248 248 248 248-111 248-248S393 8 256 8zm0 448c-110.5 0-200-89.5-200-200S145.5 56 256 56s200 89.5 200 200-89.5 200-200 200zm61.8-104.4l-84.9-61.7c-3.1-2.3-4.9-5.9-4.9-9.7V116c0-6.6 5.4-12 12-12h32c6.6 0 12 5.4 12 12v141.7l66.8 48.6c5.4 3.9 6.5 11.4 2.6 16.8L334.6 349c-3.9 5.3-11.4 6.5-16.8 2.6z"></path></svg>
{{ post.date   }}
       </time>
        <div class="mb-4 entry-content">
        
{{ post.content   }}

</div>




{% include 'content/share_social.twig' %}

    {% include "components/grid/related_articles.twig" with {
        title       : "Recommended for you",
    } %}


{% include 'content/comment.twig' %}



   </main>
</div>
		
	</div>


{% endblock %}


